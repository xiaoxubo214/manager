<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wind.dao.CoupletDao">

	<select id="queryObject" resultType="com.wind.entity.CoupletEntity">
		select * from couplet where id = #{id}
	</select>

	<select id="queryList" resultType="com.wind.entity.CoupletEntity">
		select * from couplet where theme = #{type}
	</select>

</mapper>